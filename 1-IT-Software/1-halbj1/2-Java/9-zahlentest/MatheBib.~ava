public class MatheBib{
  static double nullstelleLinear(double m, double c){
    return -1*c/m;
    }
  
  static double nst1Quadrat(double a, double b, double c){
    return ((-1*b)+Math.sqrt((b*b)-4*a*c))/(2*a);
    }
  
  static double nst2Quadrat(double a, double b, double c){
    return ((-1*b)-Math.sqrt((b*b)-4*a*c))/(2*a);
    }
  }
