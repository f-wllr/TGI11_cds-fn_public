public class zahlentest {
  
  public static void main(String[] args) {
    int zahl1 = 2147483647;
    int zahl2 = 1;
    int ergebnis = zahl1 + zahl2;
    System.out.println(ergebnis);
    
    long l = 998877665544332211l;
    System.out.println(l);
    
    double d1 = 0.1;
    double d2 = 0.2;
    System.out.println(d1 + d2);
    double summe = d1 + d2;
    System.out.println(summe == 0.3);
    
    for (int i = 0; i < i + 1 ; i++ ) {
      System.out.print("HAHAHA");
    } // end of for
    
  } // end of main

} // end of class zahlentest

